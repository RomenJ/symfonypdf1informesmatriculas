{% block title %}Alumno{% endblock %}

{% block body %}
    <h1>Alumno</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ alumno.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ alumno.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
